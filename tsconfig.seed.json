    "outDir": "./dist/seed",
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2020",
    "sourceMap": true
  },
  "include": [
    "seed/**/*",
    "lib/**/*",
    "models/**/*",
    "products.json"
  ],
  "exclude": ["node_modules", ".next"]
}